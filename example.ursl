func $main {
    const 0
    call $fib
    out %NUMB
}

func $fib : 1 -> 1 {
    get 0
    const 0
    equal
    branch :base_case

    get 0
    const 1
    equal
    branch :base_case

    get 0
    const 1
    sub
    call $fib

    get 0
    const 2
    sub
    call $fib

    add
    ret

    height 0
    :base_case
    get 0
    ret
}

